<Window x:Class="ArduinoDashboardInterpreter.ChangeScreenType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArduinoDashboardInterpreter"
        mc:Ignorable="d"
        Title="Change Screen" Height="150" Width="250" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <StackPanel Margin="20" Orientation="Vertical" VerticalAlignment="Center">
        <ComboBox Name="TypeList"/>
        <Button Margin="20 15 20 0" Click="Print_Click" Height="25">PRINT</Button>
    </StackPanel>
</Window>
